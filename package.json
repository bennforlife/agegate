{
  "name": "agegate",
  "version": "1.0.0",
  "description": "gh-pages",
  "main": "index.html",
  "scripts": {
    "jade": "jade src/index.jade -o . -P",
    "stylus": "stylus -u autoprefixer-stylus -c src/index.styl -o .",
    "build": "npm run jade && npm run stylus && npm run js",
    "develop": "watch 'npm run build' ./src",
    "js": "browserify -g uglifyify src/index.js -o agegate.min.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/izolate/agegate.git"
  },
  "author": "izolate <yoshtalwar@gmail.com> (http://izolate.net/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/izolate/agegate/issues"
  },
  "homepage": "https://github.com/izolate/agegate",
  "devDependencies": {
    "agegate": "http://github.com/izolate/agegate/tarball/master",
    "autoprefixer-stylus": "^0.5.0",
    "browserify": "^9.0.8",
    "jade": "^1.9.2",
    "stylus": "^0.50.0",
    "uglifyify": "^3.0.1",
    "watch": "^0.16.0"
  }
}
